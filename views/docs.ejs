        display: none;
    }
    #docsPage .search-docs-head{
        width: 95%;
        margin: 20px auto;
        margin-top: 50px;
    }
    #docsPage .search-docs-head .navbar .form-inline .input-group .input-group-prepend .input-group-text{
        background: var(--mit-color);
        color: var(--revers-white);
    }
    #docsPage .search-not-found{
        display: none;
        width: 30%;
        margin: 50px auto 100px auto;
    }
    #docsPage .search-not-found img{
        position: relative;
        width: 100%;
        transition: 1s ease-in-out;
    }
    #docsPage #docsList .drop{
        margin: 20px auto;
    }
    #docsPage #docsList .drop .flx .left-web,
    #docsPage #docsList .drop .flx .right-api{
        width: 60%;
        padding: 4px 20px;
        font-size: 14px;
        color: grey;
        text-align: justify;
    }
    #docsPage #docsList .drop .flx .right-web,
    #docsPage #docsList .drop .flx .left-api{
        width: 40%;
    }
    #docsPage #docsList .drop .flx .left-web h2,
    #docsPage #docsList .drop .flx .right-api h2{
        color: var(--revers-white);
        font-family: 'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif;
        text-align: left;
    }
    #docsPage #docsList .drop .flx .left-web h2 span,
    #docsPage #docsList .drop .flx .right-api h2 span{
        font-weight: bold;
        color: var(--charm);
    }
    #docsPage #docsList .drop .btn{
        margin: 0px 10px 20px auto;
    }
    #docsPage #docsList .drop .flx .left-web ul,
    #docsPage #docsList .drop .flx .right-api ul{
        margin: 2px auto 10px auto;
    }
    #docsPage #docsList .drop .flx .left-api .code-editor{
        width: 80%;
        margin: 0px;
        max-height: 280px;
        min-height: auto;
    }
    #docsPage #docsList .drop .flx .left-api .code-editor .code-wrapper{
        height: 280px;
        max-height: 280px;
    }
    #docsPage #docsList .drop .flx .left-api .sample-code{
        padding: 10px 0px;
    }
    #docsPage #docsList .drop .flx .left-api .sample-code .code-option{
        cursor: pointer;
    }
    .code-option{
        width: 10%;
        background: var(--scrollbarBG);
        color: slategrey;
        list-style: none;
        height: 100%;
        padding: 0px;
        border-radius: 5px;
        transition: 1s ease-in-out;
    }
    .code-option li{
        width: 100%;
        height: 25%;
        text-align: center;
        font-size: 20px;
        border-bottom: 1px solid slategrey;
        padding: 20px 0px;
    }
    .code-option li:last-child{
        border: none;
    }
    #docsPage #docsList .drop .flx .right-web .desktop-view{
        width: 100%;
        height: 100%;
        cursor: pointer;
    }
    #docsPage #docsList .drop .flx .right-web .desktop-view iframe{
        width: 80%;
        height: 60%;
        min-height: 300px;
        margin: 10px 10%;
        border-radius: 5px;
        border: 1px solid grey;
    }
    #docsPage #docsList .drop .flx .right-web .desktop-view:before{
        content: '';
        width: 550px;
        height: 300px;
        background: transparent !important;
        opacity: 0.4;
        position: absolute;
        right: 2%;
        margin-top: 10px;
    }
    #docsPage #docsList .drop>.right-web~.btn,
    #docsPage #docsList .drop:has(.left-web) .btn{
        margin-left: 20px;
    }
    #docsPage #docsList .drop>.right-api~.btn,
    #docsPage #docsList .drop:has(.left-api) .btn{
